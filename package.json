{
  "name": "sfdx-plugin-source-read",
  "description": "sfdx plugin to read Metadata via CRUD Metadata API",
  "version": "0.0.0-development",
  "author": "Matthias Rolke @amtrack",
  "bugs": "https://github.com/amtrack/sfdx-plugin-source-read/issues",
  "bin": {
    "sfdx-plugin-source-read": "bin/run"
  },
  "dependencies": {
    "@salesforce/command": "5.2.16",
    "@salesforce/source-deploy-retrieve": "7.4.0",
    "tslib": "2.4.0"
  },
  "devDependencies": {
    "@oclif/test": "2.2.8",
    "@types/mocha": "10.0.1",
    "chai": "4.3.6",
    "execa": "5.1.1",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "oclif": "3.2.24",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=14.16"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/amtrack/sfdx-plugin-source-read",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "additionalHelpFlags": [
      "-h"
    ]
  },
  "repository": "amtrack/sfdx-plugin-source-read",
  "scripts": {
    "build": "rm -rf lib && tsc -p . && oclif manifest",
    "prepack": "yarn build",
    "prepare": "yarn build",
    "test": "nyc --reporter=lcov --reporter=text mocha --require ts-node/register \"test/**/*.test.ts\"",
    "test:e2e": "mocha --require ts-node/register --timeout 60000 \"test/**/*.e2e.ts\""
  }
}
